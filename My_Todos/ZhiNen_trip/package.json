{
  "name": "zhinen-trip",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm exec vite",
    "start": "pnpm exec vite",
    "build": "pnpm exec vite build",
    "build:prod": "pnpm exec vite build --mode production",
    "build:analyze": "ANALYZE=true pnpm exec vite build",
    "build:test": "node build-test.js",
    "build:report": "pnpm exec vite build && npx serve dist",
    "preview": "pnpm exec vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "performance:test": "node -e \"import('./src/utils/performanceTest.js').then(m => m.quickPerformanceTest())\"",
    "performance:mid": "node -e \"import('./src/utils/midPriorityTest.js').then(m => m.quickMidPriorityTest())\"",
    "size:analyze": "vite build --mode analyze",
    "vercel-build": "npm run build"
  },
  "dependencies": {
    "@react-vant/icons": "^0.1.0",
    "axios": "^1.11.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lib-flexible": "^0.3.2",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1",
    "react-vant": "^3.3.5",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.4.47",
    "postcss-pxtorem": "^6.1.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "terser": "^5.43.1",
    "vite": "^6.3.5",
    "vite-plugin-mock": "^3.0.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "packageManager": "pnpm@9.15.2",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "overrides": {
    "@eslint-community/eslint-utils": "^4.4.0"
  }
}
