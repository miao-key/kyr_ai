{
  "version": 2,
  "buildCommand": "pnpm install --registry=https://registry.npmmirror.com/ && pnpm build",
  "outputDirectory": "dist",
  "installCommand": "pnpm install --registry=https://registry.npmmirror.com/ --network-timeout=300000",
  "devCommand": "pnpm dev",
  "framework": "vite",
  "functions": {},
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}