{
  "version": 2,
  "buildCommand": "npm install --legacy-peer-deps --registry=https://registry.npmmirror.com/ && npm run build:prod",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps --registry=https://registry.npmmirror.com/",
  "devCommand": "vite",
  "framework": null,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NPM_CONFIG_REGISTRY": "https://registry.npmmirror.com/",
    "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
    "NPM_CONFIG_FORCE": "false"
  }
}